{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "removeComments": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": false,
    "jsx": "react",
    "allowJs": false,
    "baseUrl": "./",
    "outDir": "build/",
    "paths": {},
    "types": []
  }
}
